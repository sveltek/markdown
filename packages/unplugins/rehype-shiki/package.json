{
  "name": "@sveltek/rehype-shiki",
  "version": "0.30.0",
  "author": "Sveltek",
  "description": "A custom Rehype plugin for Shiki.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltek/markdown.git",
